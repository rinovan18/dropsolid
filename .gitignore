# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/docroot/core/
/docroot/modules/contrib/
/docroot/modules/custom/
/docroot/themes/contrib/
/docroot/themes/custom/
/docroot/profiles/contrib/
/docroot/profiles/dropsolid_rocketship_profile/
/docroot/libraries/
/docroot/apc.php

# Ignore sensitive information
/docroot/sites/*/settings.php
/docroot/sites/*/settings.local.php
/docroot/sites/default/services.yml
# Ignore Drupal's file directory
/docroot/sites/*/files/

# Ignore SimpleTest multi-site environment.
/docroot/sites/simpletest

# Ignore osx file fingerprints
.DS_Store

# Ignore gitlab access token commits
auth.json
.idea
config/
!config/sync/.gitkeep
!config/splits/dev/.gitkeep
!config/splits/live/.gitkeep
!config/splits/whitelist/.gitkeep
!config/splits/staging/.gitkeep
!config/splits/local/.gitkeep

private/files
!private/files/.gitkeep

# Ignore settings_local, is generated by infra
/etc/drupal/settings_local.php
/docker-compose.*.yml

# Launchpad .gitignore begin DO NOT REMOVE OR EDIT THIS LINE
/.docker-data
/.docker-sync
/.env
/docker-compose.yml
/docker-compose.override.yml
/docker-sync.yml
/docroot/sites/default/*.local.php
/docroot/sites/default/*.aliases.drushrc.php
.sync_check_*
*.unison.tmp
# Launchpad .gitignore end DO NOT REMOVE OR EDIT THIS LINE